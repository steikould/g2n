# G2N Environment Configuration Template
# Copy to .env and fill in values. NEVER commit .env to source control.

# Environment
G2N_ENV=dev

# S3 / Medallion Architecture
G2N_S3_BRONZE_PATH=s3a://g2n-data/bronze/
G2N_S3_SILVER_PATH=s3a://g2n-data/silver/
G2N_S3_QUARANTINE_PATH=s3a://g2n-data/quarantine/
G2N_S3_FEATURES_PATH=s3a://g2n-data/features/

# Snowflake (Gold Layer)
G2N_SNOWFLAKE_ACCOUNT=your_account
G2N_SNOWFLAKE_USER=your_user
G2N_SNOWFLAKE_WAREHOUSE=G2N_WH
G2N_SNOWFLAKE_DATABASE=G2N_GOLD
G2N_SNOWFLAKE_SCHEMA=PUBLIC
G2N_SNOWFLAKE_ROLE=G2N_ROLE

# Collibra
G2N_COLLIBRA_BASE_URL=https://collibra.yourcompany.com/rest/2.0
G2N_COLLIBRA_USERNAME=your_user
G2N_COLLIBRA_COMMUNITY_ID=

# MLflow
G2N_MLFLOW_TRACKING_URI=http://localhost:5000
G2N_MLFLOW_EXPERIMENT_NAME=g2n-default

# Serving
G2N_SERVING_HOST=0.0.0.0
G2N_SERVING_PORT=8000
